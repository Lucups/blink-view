{
    "name": "blink/view",
    "description": "support view for blink framework",
    "keywords": [
        "blink",
        "view"
    ],
    "authors": [
        {
            "name": "Tony Lu",
            "email": "lucups@live.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "blink/blink": "0.1.*",
        "twig/twig": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "files": [
            "src/support/helpers.php"
        ],
        "psr-4": {
            "blink\\": "src",
            "blink\\tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    }
}
